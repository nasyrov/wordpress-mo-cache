{
    "name": "nasyrov/wordpress-mo-cache",
    "type": "wordpress-muplugin",
    "description": "WordPress mu-plugin for a faster load_textdomain.",
    "keywords": [
        "wordpress",
        "mu-plugin",
        "cache",
        "mo"
    ],
    "homepage": "https://github.com/nasyrov/wordpress-mo-cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgenii Nasyrov",
            "email": "inasyrov@ya.ru",
            "homepage": "https://github.com/nasyrov",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "~1.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~3.2"
    },
    "scripts": {
        "lint": "phpcs -p --standard=psr2 --ignore=vendor ."
    },
    "config": {
        "sort-packages": true
    }
}
